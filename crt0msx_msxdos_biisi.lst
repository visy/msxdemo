                              1         ;--- crt0.s for MSX-DOS - by Konami Man & Avelino, 11/2004
                              2         ;    Simple version: allows "void main()" only.
                              3         ;    Overhead: 6 bytes.
                              4         ;
                              5         ;    Compile programs with --code-loc 0x106 --data-loc X
                              6         ;    X=0  -> global vars will be placed immediately after code
                              7         ;    X!=0 -> global vars will be placed at address X
                              8         ;            (make sure that X>0x100+code size)
                              9 
                             10         .globl  _main
                             11 
                             12         .area _HEADER (ABS)
                             13 
   0100                      14         .org    0x0100  ;MSX-DOS .COM programs start address
                             15 
                             16         ;--- Initialize globals and jump to "main"
                             17 
   0100 C3 06 04             18         jp init
